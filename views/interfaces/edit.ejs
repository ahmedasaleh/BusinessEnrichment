<% include ../partials/header %>
<div class="container">
    <div class="row">
        <h1 style="text-align: center">Edit Interface</h1>
        <div style="width: 30%; margin: 25px auto;">
            <form action="/interfaces/<%= interface._id %>?_method=PUT" method="POST">
                <div class="form-group">
                    <label for="interfacename">Interface Name</label>
                    <input class="form-control" type="text" name="interface[ifName]" value="<%= interface.ifName %>" readonly>
                </div>
                <div class="form-group">
                    <label for="description">Interface Description</label>
                    <input class="form-control" type="text" name="interface[ifDescr]" value="<%= interface.ifDescr %>" readonly>
                </div>
                <div class="form-group">
                    <label for="alias">Interface Alias</label>
                    <input class="form-control" type="text" name="interface[ifAlias]" value="<%= interface.ifAlias %>" readonly>
                </div>
                <div class="form-group">
                    <label for="index">Interface Index</label>
                    <input class="form-control" type="text" name="interface[ifIndex]" value="<%= interface.ifIndex %>" readonly>
                </div>
                <div class="form-group">
                    <label for="type">Interface Type</label>
                    <input class="form-control" type="text" name="interface[ifType]" value="<%= interface.ifType %>" readonly>
                </div>
                <div class="form-group">
                    <label for="speed">Interface Speed</label>
                    <input class="form-control" type="text" name="interface[ifSpeed]" value="<%= interface.ifSpeed %>" readonly>
                </div>
                <div class="form-group">
                    <label for="speed">Interface High Speed</label>
                    <input class="form-control" type="text" name="interface[ifHighSpeed]" value="<%= interface.ifHighSpeed %>" readonly>
                </div>
                <div class="form-group">
                    <label for="devicename">Device Name</label>
                    <input class="form-control" type="text" name="interface[device][hostname]" value="<%= interface.hostname %>" readonly>
                </div>
                <div class="form-group">
                    <label for="actualspeed">Interface Actual Speed</label>
                    <input class="form-control" type="text" name="interface[actualspeed]" value="<%= interface.actualspeed %>">
                </div>
                <div id="sp_connType" class="form-group" >
                    <label for="sp_connType">Connection Type</label>
                    <input class="form-control" type="text" name="interface[sp_connType]" value="<%= interface.sp_connType %>">
                </div>
                <div id="sp_bundleId" class="form-group" >
                    <label for="sp_bundleId">Bundle Id</label>
                    <input class="form-control" type="text" name="interface[sp_bundleId]" value="<%= interface.sp_bundleId %>">
                </div>
                <div id="sp_linkNumber" class="form-group" >
                    <label for="sp_linkNumber">Link Number</label>
                    <input class="form-control" type="text" name="interface[sp_linkNumber]" value="<%= interface.sp_linkNumber %>">
                </div>
                <div id="sp_provider" class="form-group" >
                    <label for="sp_provider">Provider</label>
                    <input class="form-control" type="text" name="interface[sp_provider]" value="<%= interface.sp_provider %>">
                </div>
                <div id="sp_service" class="form-group" >
                    <label for="sp_service">Service</label>
                    <input class="form-control" type="text" name="interface[sp_service]" value="<%= interface.sp_service %>">
                </div>
                <div id="sp_subCable" class="form-group" >
                    <label for="sp_subCable">Sub Cable</label>
                    <input class="form-control" type="text" name="interface[sp_subCable]" value="<%= interface.sp_subCable %>">
                </div>
                <div id="sp_CID" class="form-group" >
                    <label for="sp_CID">Circuit ID</label>
                    <input class="form-control" type="text" name="interface[sp_CID]" value="<%= interface.sp_CID %>">
                </div>
                <div id="sp_TECID" class="form-group" >
                    <label for="sp_TECID">TE CID</label>
                    <input class="form-control" type="text" name="interface[sp_TECID]" value="<%= interface.sp_TECID %>">
                </div>
                <div id="sp_termination" class="form-group" >
                    <label for="sp_termination">Termination</label>
                    <input class="form-control" type="text" name="interface[sp_termination]" value="<%= interface.sp_termination %>">
                </div>
                <div id="sp_speed" class="form-group" >
                    <label for="sp_speed">Service Speed</label>
                    <input class="form-control" type="text" name="interface[sp_speed]" value="<%= interface.sp_speed %>">
                </div>
                <div id="sp_customer" class="form-group" >
                    <label for="sp_customer">Customer</label>
                    <input class="form-control" type="text" name="interface[sp_customer]" value="<%= interface.sp_customer %>">
                </div>
                <div id="sp_pop" class="form-group" >
                    <label for="sp_pop">Service Provider POP</label>
                    <input class="form-control" type="text" name="interface[sp_pop]" value="<%= interface.sp_pop %>">
                </div>
                <div id="sp_connectedBW" class="form-group" >
                    <label for="sp_connectedBW">Connected Bandwidth</label>
                    <input class="form-control" type="text" name="interface[sp_connectedBW]" value="<%= interface.sp_connectedBW %>">
                </div>
                <div id="sp_emsOrder" class="form-group" >
                    <label for="sp_emsOrder">EMS Order</label>
                    <input class="form-control" type="text" name="interface[sp_emsOrder]" value="<%= interface.sp_emsOrder %>">
                </div>
                <div id="sp_fwType" class="form-group" >
                    <label for="sp_fwType">Firewall Type</label>
                    <input class="form-control" type="text" name="interface[sp_fwType]" value="<%= interface.sp_fwType %>">
                </div>
                <div id="sp_serviceType" class="form-group" >
                    <label for="sp_serviceType">Service Type</label>
                    <input class="form-control" type="text" name="interface[sp_serviceType]" value="<%= interface.sp_serviceType %>">
                </div>
                <div id="sp_ipType" class="form-group" >
                    <label for="sp_ipType">IP Type</label>
                    <input class="form-control" type="text" name="interface[sp_ipType]" value="<%= interface.sp_ipType %>">
                </div>
                <div id="sp_vendor" class="form-group" >
                    <label for="sp_vendor">Vendor</label>
                    <input class="form-control" type="text" name="interface[sp_vendor]" value="<%= interface.sp_vendor %>">
                </div>
                <div id="sp_sourceCore" class="form-group" >
                    <label for="sp_sourceCore">Source Core</label>
                    <input class="form-control" type="text" name="interface[sp_sourceCore]" value="<%= interface.sp_sourceCore %>">
                </div>
                <div id="sp_destCore" class="form-group" >
                    <label for="sp_destCore">Destination Core</label>
                    <input class="form-control" type="text" name="interface[sp_destCore]" value="<%= interface.sp_destCore %>">
                </div>
                <div id="sp_siteCode" class="form-group" >
                    <label for="sp_siteCode">Site Code</label>
                    <input class="form-control" type="text" name="interface[sp_siteCode]" value="<%= interface.sp_siteCode %>">
                </div>
                <div id="sp_preNumber" class="form-group" >
                    <label for="sp_preNumber">Pre Number</label>
                    <input class="form-control" type="text" name="interface[sp_preNumber]" value="<%= interface.sp_preNumber %>">
                </div>
                <div id="sp_portID" class="form-group" >
                    <label for="sp_portID">Port ID</label>
                    <input class="form-control" type="text" name="interface[sp_portID]" value="<%= interface.sp_portID %>">
                </div>
                <div id="secondHost" class="form-group" >
                    <label for="secondHost">Second Host</label>
                    <input class="form-control" type="text" name="interface[secondHost]" value="<%= interface.secondHost %>">
                </div>
                <div id="secondPOP" class="form-group" >
                    <label for="secondPOP">Second POP</label>
                    <input class="form-control" type="text" name="interface[secondPOP]" value="<%= interface.secondPOP %>">
                </div>
                <div id="secondInterface" class="form-group" >
                    <label for="secondInterface">Second Interface</label>
                    <input class="form-control" type="text" name="interface[secondInterface]" value="<%= interface.secondInterface %>">
                </div>
                <div id="pop" class="form-group" >
                    <label for="pop">POP</label>
                    <input class="form-control" type="text" name="interface[pop]" value="<%= interface.pop %>">
                </div>
                <div class="form-group">
                    <label for="isUpLink">Is UpLink</label>
                    <select class="form-control" type="text" name="interface[isUpLink]" value="<%= interface.isUpLink %>">
                    <% if(interface.isUpLink === undefined ) { %>
                        <option value="1">1</option>
                        <option value="0" selected="selected">0</option>
                    <% } else if( parseInt(interface.isUpLink) == 0) {%>
                        <option value="1">1</option>
                        <option value="0" selected="selected">0</option>
                    <% } else {%>
                        <option value="1" selected="selected">1</option>
                        <option value="0">0</option>
                        <% } %>
                    </select>
                </div>
                <div class="form-group">
                    <label for="provisoFlag">To Be Exported to TNPM</label>
                    <select class="form-control" type="text" name="interface[provisoFlag]" value="<%= interface.provisoFlag %>">
                    <% if(interface.provisoFlag === undefined ) { %>
                        <option value="1">1</option>
                        <option value="0" selected="selected">0</option>
                    <% } else if( parseInt(interface.provisoFlag) == 0) {%>
                        <option value="1">1</option>
                        <option value="0" selected="selected">0</option>
                    <% } else {%>
                        <option value="1" selected="selected">1</option>
                        <option value="0">0</option>
                        <% } %>
                    </select>
                </div>
                <div id="linkType" class="form-group" >
                    <label for="linkType">Link Type</label>
                    <select class="form-control" type="text" name="interface[type]" value="<%= interface.type %>">
                    <% if(interface.type === undefined) { %>
                        <option value="Local" >Local</option>
                        <option value="WAN">WAN</option>
                        <option value="NONE" selected="selected">NONE</option>
                    <% } else if(interface.type == null) {%>
                        <option value="NONE" selected="selected">NONE</option>
                        <option value="Local" >Local</option>
                        <option value="WAN">WAN</option>
                    <% } else if(interface.type == "Local") {%>
                        <option value="Local" selected="selected">Local</option>
                        <option value="WAN">WAN</option>
                        <option value="NONE">NONE</option>
                    <% } else if(interface.type == "WAN"){%>
                        <option value="WAN" selected="selected">WAN</option>
                        <option value="Local">Local</option>
                        <option value="NONE">NONE</option>
                        <% } %>
                    </select>
                </div>
                <div id="pollInterval" class="form-group" >
                    <label for="pollInterval">Poll Interval</label>
                    <select class="form-control" type="text" name="interface[pollInterval]" value="<%= interface.pollInterval %>">
                    <% if(interface.pollInterval === "15min") { %>
                        <option value="15min" selected="selected">"15min"</option>
                        <option value="02min"> "02min"</option>
                    <% } else {%>
                        <option value="02min" selected="selected"> "02min"</option>
                        <option value="15min" >"15min"</option>
                        <% } %>
                    </select>
                </div>
                <div id="newfield" class="form-group" >
                    <label for="newfield">new field</label>
                    <input class="form-control" type="text" name="interface[newfield]" value="<%= interface.newfield %>">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Update</button>
                </div>
            </form>
        </div>
    </div>
</div>
<% include ../partials/footer %>

